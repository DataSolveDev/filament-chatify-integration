{
    "name": "monzer/filament-chatify-integration",
    "description": "Seamlessly integrate real-time messaging into your application.",
    "type": "library",
    "license": "MIT",
    "version": "1.0.1",
    "autoload": {
        "psr-4": {
            "Monzer\\FilamentChatifyIntegration\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Monzer Osman",
            "email": "monzerosman@live.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/support": "^3.0",
        "munafio/chatify": "dev-master",
        "spatie/laravel-package-tools": "^1.9"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/DataSolveDev/chatify"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Monzer\\FilamentChatifyIntegration\\ChatifyServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
